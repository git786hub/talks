[{"id":"f5c05c8d-bff7-11e5-82d3-f74a82f26141","name":"Unnamed document","folder":"general","content":"// Create a node\nCREATE (n {name:\"World\"}) RETURN \"hello\", n.name"},{"id":"f5c05c8d-bff7-11e5-82d4-f74a82f26141","name":"Unnamed document","folder":"general","content":"// Get some data\nMATCH (n) RETURN n LIMIT 100"},{"id":"f5c05c9c-bff7-11e5-82d4-f74a82f26141","name":"Unnamed document","folder":"general","content":"// What is related, and how\nMATCH (a)-[r]->(b)\nWHERE labels(a) <> [] AND labels(b) <> []\nRETURN DISTINCT head(labels(a)) AS This, type(r) as To, head(labels(b)) AS That\nLIMIT 10"},{"id":"f5c05c9c-bff7-11e5-82d5-f74a82f26141","name":"Unnamed document","folder":"general","content":"// REST API\n:GET /db/data"},{"id":"f5c05c9c-bff7-11e5-82d6-f74a82f26141","name":"Unnamed document","folder":"system","content":"// Server configuration\n:GET /db/manage/server/jmx/domain/org.neo4j/instance%3Dkernel%230%2Cname%3DConfiguration"},{"id":"f5c05c9c-bff7-11e5-82d7-f74a82f26141","name":"Unnamed document","folder":"system","content":"// Kernel information\n:GET /db/manage/server/jmx/domain/org.neo4j/instance%3Dkernel%230%2Cname%3DKernel"},{"id":"f5c05ca0-bff7-11e5-82d7-f74a82f26141","name":"Unnamed document","folder":"system","content":"// ID Allocation\n:GET /db/manage/server/jmx/domain/org.neo4j/instance%3Dkernel%230%2Cname%3DPrimitive%20count"},{"id":"f5c05ca0-bff7-11e5-82d8-f74a82f26141","name":"Unnamed document","folder":"system","content":"// Store file sizes\n:GET /db/manage/server/jmx/domain/org.neo4j/instance%3Dkernel%230%2Cname%3DStore%20file%20sizes"},{"id":"f5c05cae-bff7-11e5-82d8-f74a82f26141","name":"Unnamed document","folder":"system","content":"// Extensions\n:GET /db/data/ext"},{"id":"5b165ab3-c94b-11e5-a4ec-d10326664d4d","name":"Unnamed document","folder":"5d2f9dc2-c94b-11e5-a4ec-d10326664d4d","content":"// Clear out the DB and start fresh\nMATCH (n)\nDETACH DELETE n;"},{"id":"7c8aa7a8-c94b-11e5-bf38-a9aacfbe617c","name":"Unnamed document","folder":"5d2f9dc2-c94b-11e5-a4ec-d10326664d4d","content":"// Create some products\nCREATE (:Product {product_id: 12, name: \"Dog food\", description: \"The most delicious dog food ever\", price: 20}),\n (:Product {product_id: 34, name: \"Dog biscuits\", description: \"The yummiest dog biscuits ever\", price: 5}),\n (:Product {product_id: 56, name: \"Squeaky dog toy\", description: \"A fun squeaky toy\", price: 10}),\n (:Product {product_id: 78, name: \"Comfy cat bed\", description: \"Soft and claw-proof\", price: 30}),\n (:Product {product_id: 90, name: \"Sunglasses\", description: \"It has nothing to do with pets\", price: 15});"},{"id":"8f3a490a-c94b-11e5-bf38-a9aacfbe617c","name":"Unnamed document","folder":"5d2f9dc2-c94b-11e5-a4ec-d10326664d4d","content":"// Create some customers\nCREATE (alice:Customer {name: \"Alice\", email: \"alice@example.com\"}),\n (bob:Customer {name: \"Bob\", email: \"bob@example.com\"}),\n (:Customer {name: \"Charlie\", email: \"charlie@example.com\"}),\n (:Customer {name: \"Dave\", email: \"dave@example.com\"})\nRETURN alice, bob;"},{"id":"ab6aedf1-c94b-11e5-b149-dfc650904499","name":"Unnamed document","folder":"5d2f9dc2-c94b-11e5-a4ec-d10326664d4d","content":"// Create an order for Alice\nMATCH (customer:Customer), (product:Product)\nWHERE customer.email = \"alice@example.com\" AND product.product_id IN [12, 34]\nMERGE (customer)-[:PLACED]->(order:Order {date: \"2016-02-05\"})\nMERGE (order)-[:CONTAINS]->(product);"},{"id":"bdaf4a64-c94b-11e5-b149-dfc650904499","name":"Unnamed document","folder":"5d2f9dc2-c94b-11e5-a4ec-d10326664d4d","content":"// Create an order for Bob \nMATCH (customer:Customer {email: \"bob@example.com\"}),\n (product1:Product {product_id: 34}),\n (product2:Product {product_id: 56})\nMERGE (customer)-[:PLACED]->(order:Order {date: \"2016-02-05\"})\nMERGE (order)-[:CONTAINS]->(product1)\nMERGE (order)-[:CONTAINS]->(product2);"},{"id":"cb0300da-c94b-11e5-b149-dfc650904499","name":"Unnamed document","folder":"5d2f9dc2-c94b-11e5-a4ec-d10326664d4d","content":"// Create an order for Charlie \nMATCH (customer:Customer {email: \"charlie@example.com\"}), (product:Product)\nWHERE product.product_id IN [12, 34, 56, 90]\nMERGE (customer)-[:PLACED]->(order:Order {date: \"2016-02-05\"})\nMERGE (order)-[:CONTAINS]->(product);"},{"id":"d8bc08c0-c94b-11e5-b149-dfc650904499","name":"Unnamed document","folder":"5d2f9dc2-c94b-11e5-a4ec-d10326664d4d","content":"// Create an order for Dave\nMATCH (customer:Customer {email: \"dave@example.com\"}),\n (product1:Product {product_id: 78}),\n (product2:Product {product_id: 90})\nMERGE (customer)-[:PLACED]->(order:Order {date: \"2016-02-05\"})\nMERGE (product1)<-[:CONTAINS]-(order)-[:CONTAINS]->(product2);"},{"id":"e0c27a44-c94b-11e5-b149-dfc650904499","name":"Unnamed document","folder":"5d2f9dc2-c94b-11e5-a4ec-d10326664d4d","content":"// Let's see what we've created\nMATCH (n)\nRETURN n\nLIMIT 50;"},{"id":"0ea61114-c94c-11e5-b149-dfc650904499","name":"Unnamed document","folder":"5d2f9dc2-c94b-11e5-a4ec-d10326664d4d","content":"// Anonymous recommendation\nMATCH (order:Order)-[:CONTAINS]->(product:Product {product_id: 34}),\n (order)-[:CONTAINS]->(recommendation:Product)\nWHERE NOT recommendation = product\nRETURN recommendation.name, COUNT(recommendation) as recommended\nORDER BY recommended DESC;"},{"id":"98525c2d-c94c-11e5-8345-33ee2baf2d69","name":"Unnamed document","folder":"5d2f9dc2-c94b-11e5-a4ec-d10326664d4d","content":"// Recommendation for Alice\nMATCH (customer:Customer)-[:PLACED]->(order:Order)-[:CONTAINS]->(product:Product {product_id: 34}),\n (order)-[:CONTAINS]->(recommendation:Product)\nWHERE NOT recommendation = product AND NOT customer.email = \"alice@example.com\"\nRETURN recommendation.name, COUNT(recommendation) as recommended\nORDER BY recommended DESC;"},{"id":"df8a0809-c950-11e5-958d-3386d0778577","name":"Unnamed document","folder":"bb4f6f26-c950-11e5-958d-3386d0778577","content":"// Graph the bridges\nCREATE (west:Land {name: \"West\"}),\n (north:Land {name: \"North\"}),\n (east:Land {name: \"East\"}),\n (south:Land {name: \"South\"}),\n\n (west)-[b1:BRIDGE {name: \"West to North (1)\"}]->(north),\n (west)-[b2:BRIDGE {name: \"West to North (2)\"}]->(north),\n\n (north)-[b3:BRIDGE {name: \"North to East\"}]->(east),\n\n (west)-[b4:BRIDGE {name: \"West to South (1)\"}]->(south),\n (west)-[b5:BRIDGE {name: \"West to South (2)\"}]->(south),\n\n (west)-[b6:BRIDGE {name: \"West to East\"}]->(east),\n\n (south)-[b7:BRIDGE {name: \"South to East\"}]->(east)\nRETURN west, north, south, east, b1, b2, b3, b4, b5, b6, b7;"},{"id":"f5a02cf9-c950-11e5-958d-3386d0778577","name":"Unnamed document","folder":"bb4f6f26-c950-11e5-958d-3386d0778577","content":"// Go for a walk\nMATCH (:Land)-[b1:BRIDGE]-(:Land)-[b2:BRIDGE]-(:Land)-[b3:BRIDGE]-(:Land)-[b4:BRIDGE]-(:Land)-[b5:BRIDGE]-(:Land)-[b6:BRIDGE]-(:Land)-[b7:BRIDGE]-(:Land)\nRETURN b1, b2, b3, b4, b5, b6, b7;"},{"id":"0cd7a062-c951-11e5-958d-3386d0778577","name":"Unnamed document","folder":"bb4f6f26-c950-11e5-958d-3386d0778577","content":"// \"Eulerian Path\" possible if 0 or 2 have odd number of bridges\nMATCH ()-[b:BRIDGE {name: \"West to East\"}]-()\nDELETE b;"},{"id":"16907cdb-c951-11e5-958d-3386d0778577","name":"Unnamed document","folder":"bb4f6f26-c950-11e5-958d-3386d0778577","content":"// Try again with 6 bridges\nMATCH (:Land)-[b1:BRIDGE]-(:Land)-[b2:BRIDGE]-(:Land)-[b3:BRIDGE]-(:Land)-[b4:BRIDGE]-(:Land)-[b5:BRIDGE]-(:Land)-[b6:BRIDGE]-(:Land)\nRETURN b1, b2, b3, b4, b5, b6;"},{"id":"2a717245-c951-11e5-958d-3386d0778577","name":"Unnamed document","folder":"bb4f6f26-c950-11e5-958d-3386d0778577","content":"// Solve all routes for 6 bridges\nMATCH (land:Land)-[b1:BRIDGE]-(:Land)-[b2:BRIDGE]-(:Land)-[b3:BRIDGE]-(:Land)-[b4:BRIDGE]-(:Land)-[b5:BRIDGE]-(:Land)-[b6:BRIDGE]-(:Land)\nRETURN land.name, b1.name, b2.name, b3.name, b4.name, b5.name, b6.name;"}]
